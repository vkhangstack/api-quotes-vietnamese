{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "array-callback-return": "warn",
    "block-scoped-var": "warn",
    "func-call-spacing": "error",
    "func-names": "error",
    "func-style": "error",
    "no-loop-func": "error",
    "no-duplicate-imports": "error",
    "strict": "error",
    "camelcase": "error",
    "require-await": "error"
  }
}
